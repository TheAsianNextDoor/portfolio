    "node-fetch": "^3.2.8",
    "npm-check-updates": "^15.3.4",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-plugin-checker": "^0.4.9",
    "vite-plugin-environment": "^1.1.2",
